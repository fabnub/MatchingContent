                                        Header="SUBJECT" MinWidth="140"
                                        Binding="{Binding Subject, Mode=OneWay}"  />

                    <DataGridTextColumn 
                                        Header="GRADE" MinWidth="100"
                                        Binding="{Binding Grade, Mode=OneWay}" />

                </DataGrid.Columns>
            </DataGrid>
