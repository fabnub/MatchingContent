                    <ComboBoxItem>Thursday</ComboBoxItem>
                    <ComboBoxItem>Friday</ComboBoxItem>
                </ComboBox>
                <Label Content="Subject" HorizontalAlignment="Left" Margin="254,41,0,0" Grid.Row="3" VerticalAlignment="Top" Width="88"/>
