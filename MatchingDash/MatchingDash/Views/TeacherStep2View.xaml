            <TextBox Grid.Row="0" HorizontalAlignment="Left" Height="23" Margin="391,7,0,0" TextWrapping="Wrap" Controls:TextBoxHelper.Watermark="Type here" Text="{Binding LastName, Mode=TwoWay}"  VerticalAlignment="Top" Width="156"/>
