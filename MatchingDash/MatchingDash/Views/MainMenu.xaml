                            </TextBox>
                            <!--<ComboBox 
                             
                              HorizontalAlignment="Left"  
                              VerticalAlignment="Top"
                               Margin="0,5,0,0" 
                               Controls:TextBoxHelper.ClearTextButton="True"
                          Controls:TextBoxHelper.Watermark="Select Type "
